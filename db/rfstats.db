
 

############# RF stats (MSG ID 104) #########################



record(longin, "$(P){RFDFE:$(NO)}Freq-PS-IRAW") {
    field(DESC, "Measured clock frequency")
    field(DTYP, "PSC Reg")
    field(INP , "@psc$(NO) 104 0")
    field(SCAN,"I/O Intr")
    field(FLNK,"$(P){RFDFE:$(NO)}Freq-PS-I")
}

record(calc, "$(P){RFDFE:$(NO)}Freq-PS-I") {
    field(DESC, "Measured clock frequency (MHz)")
    field(INPA,"$(P){RFDFE:$(NO)}Freq-PS-IRAW")
    field(CALC,"A/1e6")
    field(PREC,"6")
}

