#############################
### ADC DMA Waveform 16K samples
#############################


record(waveform,"$(P){RFDFE:$(NO)}ADC:A:Buff-Wfm") {
    field(DESC, "117 MHz ADC0 raw waveform")
    field(DTYP, "PSC Block I16 In")
    field(INP , "@psc$(NO) 60 0 ")
    field(SCAN, "I/O Intr")
    field(FTVL, "DOUBLE")
    field(HOPR, "32768")
    field(LOPR, "-32767")
    field(NELM,"$(ADC_LEN)")
}

